<?xml version='1.0' encoding='utf-8'?>
<widget id="com.theparcl.v1" version="1.3.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Parcl</name>
    <description>
        realestatate application
    </description>
    <author email="l@emelle.me" href="http://cordova.io">
        -melle.io-
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-network-information" />
    <plugin name="cordova-plugin-inappbrowser" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="40" src="res/icon-small.png" width="40" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="StatusBarOverlaysWebView" value="false" />
    </platform>
    <preference name="orientation" value="portrait" />
    <engine name="android" spec="~5.1.1" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <universal-links>
        <host name="app.myparcl.com">
            <path event="checkBetaUser" url="/#/beta/*" />
        </host>
    </universal-links>
    <engine name="ios" spec="latest" />
    <plugin name="cordova-plugin-googlemaps" spec="https://github.com/mapsplugin/cordova-plugin-googlemaps.git#multiple_maps">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyDpajAUICmsESNG_aWaUHi66zXyeM4a9kg" />
        <variable name="API_KEY_FOR_IOS" value="AIzaSyCpdpEoVgZOtQkmtL4hyUtkZ6c2T2asnyE" />
        <variable name="NSLOCATIONWHENINUSEUSAGEDESCRIPTION" value="Show your location on the map" />
        <variable name="NSLOCATIONALWAYSUSAGEDESCRIPTION" value="Trace your location on the map" />
    </plugin>
</widget>
